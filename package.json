    "url": "https://github.com/imjihye/rechart/issues"
  },
  "homepage": "https://github.com/imjihye/rechart#readme",
  "dependencies": {
    "bootstrap": "^3.3.7",
    "bootstrap-css": "^3.0.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "jquery": "^3.2.1",
    "react": "^15.3.2",
    "react-bootstrap": "^0.30.3",
    "react-cookie": "^2.1.2",
    "react-d3-library": "^1.1.8",
    "react-d3-tooltip": "^1.4.14",
    "react-dom": "^15.3.2",

{
  "name": "rechart",
  "version": "1.0.0",
  "description": "",
  "main": "webpack.config.js",
  "scripts": {
    "clean": "rm -rf *.bundle.*",
    "build": "BUILD_TARGET=buildall webpack --progress --colors",
    "build-watch": "BUILD_TARGET=buildall webpack --progress --colors --watch",
    "build-all": "BUILD_TARGET=buildall webpack --progress --colors --watch --optimize-minimize",
    "start": "npm run clean && npm run build && NODE_ENV=production node ./server/main.js",
    "dev": "npm run clean && npm run build && PORT=3333 NODE_ENV=dev node ./server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjihye/rechart.git"
  },
